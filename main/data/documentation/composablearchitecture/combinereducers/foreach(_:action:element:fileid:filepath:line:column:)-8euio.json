leArchitecture7ReducerP5StateQa","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/State"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"ElementAction"},{"text":" == ","kind":"text"},{"kind":"typeIdentifier","text":"Element"},{"text":".","kind":"text"},{"preciseIdentifier":"s:22ComposableArchitecture7ReducerP6ActionQa","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/Action","text":"Action","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"ID","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"Hashable","preciseIdentifier":"s:SH"},{"kind":"text","text":", "},{"text":"ID","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:s8SendableP","text":"Sendable"},{"text":", ","kind":"text"},{"text":"Element","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP","text":"Reducer"},{"kind":"text","text":"\n"}]}]},{"parameters":[{"name":"toElementsState","content":[{"type":"paragraph","inlineContent":[{"text":"A writable key path from parent state to an ","type":"text"},{"code":"IdentifiedArray","type":"codeVoice"},{"text":" of child","type":"text"},{"type":"text","text":" "},{"type":"text","text":"state."}]}]},{"name":"toElementAction","content":[{"type":"paragraph","inlineContent":[{"text":"A case path from parent action to an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/IdentifiedAction"},{"text":" of child","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"actions."}]}]},{"name":"element","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that will be invoked with child actions against elements of child"},{"text":" ","type":"text"},{"type":"text","text":"state."}]}]},{"name":"fileID","content":[{"inlineContent":[{"type":"text","text":"The fileID."}],"type":"paragraph"}]},{"name":"filePath","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The filePath."}]}]},{"content":[{"inlineContent":[{"text":"The line.","type":"text"}],"type":"paragraph"}],"name":"line"},{"name":"column","content":[{"inlineContent":[{"type":"text","text":"The column."}],"type":"paragraph"}]}],"kind":"parameters"},{"content":[{"text":"Return Value","level":2,"anchor":"return-value","type":"heading"},{"inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}],"type":"paragraph"}],"kind":"content"},{"kind":"content","content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"For example, if a parent feature holds onto an array of child states, then it can perform","type":"text"},{"type":"text","text":" "},{"text":"its core logic ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"and"}]},{"type":"text","text":" the child’s logic by using the "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" operator:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["@Reducer","struct Parent {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case rows(IdentifiedActionOf<Row>)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \\.rows) {","      Row()","    }","  }","}"]},{"style":"tip","name":"Tip","type":"aside","content":[{"inlineContent":[{"type":"text","text":"We are using "},{"code":"IdentifiedArray","type":"codeVoice"},{"text":" from our","type":"text"},{"type":"text","text":" "},{"type":"reference","identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","isActive":true},{"text":" library because it provides a safe","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and ergonomic API for accessing elements from a stable ID rather than positional indices."}],"type":"paragraph"}]},{"type":"paragraph","inlineContent":[{"text":"The ","type":"text"},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"text":" ","type":"text"},{"type":"text","text":"runs the child first, and then the parent. If the order was reversed, then it would be"},{"text":" ","type":"text"},{"type":"text","text":"possible for the parent feature to remove the child state from the array, in which case the"},{"type":"text","text":" "},{"type":"text","text":"child feature would not be able to react to that action. That can cause subtle bugs."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is still possible for a parent feature higher up in the application to remove the child"},{"type":"text","text":" "},{"text":"state from the array before the child has a chance to react to the action. In such cases a","type":"text"},{"type":"text","text":" "},{"text":"runtime warning is shown in Xcode to let you know that there’s a potential problem.","type":"text"}]}]}],"references":{"doc://ComposableArchitecture/documentation/ComposableArchitecture/CombineReducers":{"abstract":[{"type":"text","text":"Combines multiple reducers into a single reducer."}],"title":"CombineReducers","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/CombineReducers","kind":"symbol","type":"topic","url":"\/documentation\/composablearchitecture\/combinereducers","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"CombineReducers","kind":"identifier"}],"navigatorTitle":[{"text":"CombineReducers","kind":"identifier"}],"role":"symbol"},"doc